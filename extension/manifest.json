{
  "manifest_version": 2,
  "name": "InStory Client",
  "version": "0.1",
  "background": {
    "scripts": [
      "./background.js"
    ]
  },
  "browser_action": {
    "default_title": "Inspiration Seeking Study"
  },
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*"
  ],
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}
